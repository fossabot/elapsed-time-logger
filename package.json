{
  "name": "elapsed-time-logger",
  "version": "1.0.4",
  "description": "Similiar to console.time() but returns readable elapsed time e.g Label: 1 hour 20 minutes 10.3 seconds",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "watch": "tsc --watch",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vltansky/elapsed-time-logger"
  },
  "bugs": {
    "url": "https://github.com/vltansky/elapsed-time-logger/issues"
  },
  "homepage": "https://github.com/vltansky/elapsed-time-logger#readme",
  "keywords": [
    "time",
    "timer",
    "elapsed",
    "span",
    "readable",
    "timing",
    "perf",
    "performance",
    "bench",
    "benchmark",
    "profiling",
    "measure",
    "seconds",
    "milliseconds",
    "nanoseconds",
    "formatted"
  ],
  "author": {
    "name": "Vlad Tansky",
    "email": "vl.tansky@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.0",
    "jest": "^25.4.0",
    "prettier": "^2.0.4",
    "ts-jest": "^25.4.0",
    "tslint": "^6.1.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.8.3"
  }
}
